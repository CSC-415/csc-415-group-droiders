<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <TextView
        android:id="@+id/leaderboardtitle"
        android:fontFamily="@font/press_start_2p"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="Leaderboard"
        android:textSize="24sp"
        android:paddingTop="25dp"
        android:layout_margin="24sp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="25dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/leaderboardtitle"
        android:id="@+id/back"
        tools:text = "BACK"
        android:fontFamily="@font/press_start_2p"
        android:textSize="12sp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="25dp"
        app:layout_constraintTop_toBottomOf="@id/leaderboardtitle"
        app:layout_constraintStart_toStartOf="@id/back"
        android:id="@+id/num1"
        tools:text = "number 1"
        android:fontFamily="@font/press_start_2p"
        android:textSize="12sp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="25dp"
        app:layout_constraintTop_toBottomOf="@id/num1"
        app:layout_constraintStart_toStartOf="@id/back"
        android:id="@+id/num2"
        tools:text = "number 2"
        android:fontFamily="@font/press_start_2p"
        android:textSize="12sp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="25dp"
        app:layout_constraintTop_toBottomOf="@id/num2"
        app:layout_constraintStart_toStartOf="@id/back"
        android:id="@+id/num3"
        tools:text = "number 3"
        android:fontFamily="@font/press_start_2p"
        android:textSize="12sp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="25dp"
        app:layout_constraintTop_toBottomOf="@id/num3"
        app:layout_constraintStart_toStartOf="@id/back"
        android:id="@+id/num4"
        tools:text = "number 4"
        android:fontFamily="@font/press_start_2p"
        android:textSize="12sp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="25dp"
        app:layout_constraintTop_toBottomOf="@id/num4"
        app:layout_constraintStart_toStartOf="@id/back"
        android:id="@+id/num5"
        tools:text = "number 5"
        android:fontFamily="@font/press_start_2p"
        android:textSize="12sp"
        />



</androidx.constraintlayout.widget.ConstraintLayout>